{
  "name": "eczane-scraper",
  "version": "1.0.0",
  "description": "Nöbetçi eczaneleri çeken web scraper uygulaması",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/index.ts",
    "scrape": "ts-node src/index.ts",
    "server": "ts-node src/server.ts",
    "server:watch": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "prod": "npm run build && npm start"
  },
  "keywords": [
    "scraper",
    "eczane",
    "pharmacy",
    "web-scraping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.5",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^3.2.0",
    "express": "^4.18.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
